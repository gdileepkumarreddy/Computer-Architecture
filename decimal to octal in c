#include<stdio.h>
int main()
{
 logn decimalnum,quotient,octalnum,remainder;
 int octalNumber[100],i=1,j;
 printf("Enter the decimal value:");
 scanf("%d",&decimalnum);
 quotient=decimalnum;
 while(quotient!=00
 {
 octalNumber[i++]=quotient%8;
 quotient=quotient/8;
 }
 for(j=i-1;j>0;j--)
 {
 octalnum=octalnum*10+octalNumber[j];
 printf("Equivalent octal value of decimal n0 %d is:%d",decimalnum,octalnum);
 }
 return 0;
 }
